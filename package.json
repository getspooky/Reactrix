{
  "name": "reactrix",
  "version": "1.0.0",
  "description": "Simple, lightweight model-based validation for React",
  "main": "dist/reactrix.cjs.js",
  "module": "dist/reactrix.esm.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "build": "kcd-scripts build --bundle",
    "setup": "npm install && npm run validate -s",
    "test": "kcd-scripts test",
    "lint": "kcd-scripts lint",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate"
  },
  "author": "Yasser Ameur El Idrissi",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  },
  "peerDependencies": {
    "react": ">=16.13"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "kcd-scripts": "^6.2.4",
    "jest-in-case": "^1.0.2",
    "react-test-renderer": "^16.13.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ]
}
